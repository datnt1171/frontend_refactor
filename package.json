{
  "name": "frontend_refactor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate:types": "openapi-typescript http://localhost:8000/api/schema/ --output src/types/openapi.d.ts",
    "generate:types_dw": "openapi-typescript http://localhost:8001/openapi.json --output src/types/openapi_dw.d.ts",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:localhost": "playwright test --project=localhost-mobile-chrome --project=localhost-desktop-chrome",
    "test:localhost:mobile": "playwright test --project=localhost-mobile-chrome --project=localhost-mobile-safari",
    "test:localhost:desktop": "playwright test --project=localhost-desktop-chrome --project=localhost-desktop-firefox",
    "test:localhost:all": "playwright test --project=localhost-*",
    "test:staging": "playwright test --project=staging-mobile-chrome --project=staging-desktop-chrome",
    "test:staging:mobile": "playwright test --project=staging-mobile-chrome --project=staging-mobile-safari", 
    "test:staging:all": "playwright test --project=staging-*",
    "test:smoke": "playwright test --project=prod-smoke",
    "test:mobile": "playwright test --project=*mobile*",
    "test:desktop": "playwright test --project=*desktop*"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-table": "^8.21.3",
    "@types/jwt-decode": "^2.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^17.2.1",
    "echarts": "^6.0.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "next-intl": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.0",
    "use-debounce": "^10.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "openapi-typescript": "^7.8.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3"
  }
}
